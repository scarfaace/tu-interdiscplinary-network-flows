{
  "version": "v2",
  "preprocessing": {
    "flows": [{
      "active_timeout": 60,
      "idle_timeout": 60,
      "features": [
        "flowStartMilliseconds",
        "flowDurationMilliseconds",
        "sourceIPAddress",
        "destinationIPAddress",
        "packetTotalCount",
        {"accumulate": ["ipTotalLength"]},
        {"accumulate": ["_interPacketTimeMilliseconds"]},
        {"accumulate": ["flowDirection"]}
      ],

      "bidirectional": true,
      "key_features": [
        "sourceIPAddress",
        "destinationIPAddress"
      ]
    }]
  }
}